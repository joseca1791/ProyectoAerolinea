<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pc.aerobarrantes">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavDrawerActivity"
            android:label="@string/title_activity_nav_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AvionesActivity"
            android:label="@string/title_activity_aviones"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ClientesActivity"
            android:label="@string/title_activity_clientes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CiudadesActivity"
            android:label="@string/title_activity_ciudades"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViajesActivity"
            android:label="@string/title_activity_viajes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VuelosActivity"
            android:label="@string/title_activity_vuelos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>