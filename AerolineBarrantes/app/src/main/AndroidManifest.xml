<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pc.aerolinebarrantes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MenuAerolinea"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.AdmAvionActivity"
            android:label="@string/title_activity_adm_avion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.AdmCiudadActivity"
            android:label="@string/title_activity_adm_ciudad"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.AdmClienteActivity"
            android:label="@string/title_activity_adm_cliente"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.AdmVueloActivity"
            android:label="@string/title_activity_adm_vuelo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.AdmViajesActivity"
            android:label="@string/title_activity_adm_viajes"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>